import javax.swing.JOptionPane;

public class ClassOrganizer {
	public static void main(String[] args) {
		String[] names = new String[6];
		for (int i =  0; i < names.length; i++) {
			names[i] = JOptionPane.showInputDialog("What is your name?");
		}
		double[] gpas = new double[6];
		for (int i = 0; i < gpas.length; i++) {
			gpas[i] = Double.parseDouble(JOptionPane.showInputDialog("What is " + names[i] + "'s GPA?"));
		}
		for (int i = gpas.length; i > 0; i--) {
			double highest = gpas[0];
			int spot = 0;
			for (int j = 1; j < i; j++) {
				if (gpas[j] > highest) {
					highest = gpas[j];
					spot = j;
				}
			}
			double temp = gpas[i - 1];
			gpas[i - 1] = highest;
			gpas[spot] = temp;
			String tempName = names[i - 1];
			names[i - 1] = names[spot];
			names[spot] = tempName;
		}
		for (int i = 0; i < names.length; i++) {
			System.out.println(names[i] + "'s GPA is " + gpas[i]);
		}
	}
}
