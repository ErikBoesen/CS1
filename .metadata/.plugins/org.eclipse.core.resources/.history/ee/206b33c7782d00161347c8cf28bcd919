import javax.swing.JOptionPane;

public class BubbleSort {
	public static void main(String[] args) {
		int[] numbers = new int[10];
		String counter = "";
		for (int i = 0; i < 10; i++) {
			numbers[i] = (int)(Math.random() * 1000 + 1);
			counter += numbers[i];
		}
		JOptionPane.showMessageDialog(null, "The random numbers are " + counter);
		// Bubble sort
		for (int i = 10; i > 0; i--) {
			int highest = numbers[0];
			int spot = 0;
			for (int j = 0; j < i; j++) {
				System.out.println("n = " + i + " i = " + j + ", prod = " + (i*j));
			}
		}
	}
}
