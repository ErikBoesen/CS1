import javax.swing.JOptionPane;

public class GolfScores {
	public static void main(String[] args) {
		int[] mason = new int[6];
		int[] clarke = new int[6];
		for (int i = 0; i < 6; i++) {
			mason[i] = Integer.parseInt(JOptionPane.showInputDialog("Enter Mason score #" + i));
		}
		for (int i = 0; i < 6; i++) {
			clarke[i] = Integer.parseInt(JOptionPane.showInputDialog("Enter Clarke score #" + i));
		}

		int mGreatest1 = mason[0];
		int mGreatest2 = mason[0];
		for (int i = 1; i < 6; i++) {
			if (mason[i] > mGreatest1) {
				mGreatest2 = mGreatest1 = mason[i];
			}
		}
		int cGreatest1 = clarke[0];
		int cGreatest2 = clarke[0];
		for (int i = 1; i < 6; i++) if (mason[i] > cGreatest) cGreatest = clarke[i];
		
		double mA = 0;
		double cA = 0;
		for (int i = 0; i < 6; i++) {
			mA += mason[i];
			cA += clarke[i];
		}
		
		mA = (mA - mGreatest) / 8.0;
		mA
		
		JOptionPane.showMessageDialog("Mason average is " + mA + ", Clarke average is " + cA + ". " + mA > cA ? "Mason had higher average scores." : "Clarke had higher average scores." );
	}
}
