import javax.swing.JOptionPane;

public class Order {
	public static void main(String[] args) {
		/*String[] fnames = new String[10];
		String[] lnames = new String[10];
		int[] grades = new int[10];
		for (int i =  0; i < fnames.length; i++) {
			fnames[i] = JOptionPane.showInputDialog("What is student #" + (i+1) + "'s first name?");
			lnames[i] = JOptionPane.showInputDialog("What is student #" + (i+1) + "'s last name?");
			grades[i] = Integer.parseInt(JOptionPane.showInputDialog("What grade is student #" + (i+1) + " in?"));
		}*/
		String[] fnames = {
				"Joe", "Jack", "Kim", "Bob", "Elizabeth", "Erik", "Josh", "Jim", "Jake", "Joey"
		};
		String[] lnames = {
				"NFshm", "KSenr", "BJunr", "AJunirr", "ISop", "PSophma", "HFrsmn", "LSenri", "WFshman", "LJunori"
		};
		int[] grades = {
				9, 12, 11, 11, 10, 10, 9, 12, 9, 11
		};
		for (int i = lnames.length; i > 0; i--) {
			if (lnames[i].charAt(0) < lnames[i - 1].charAt(0)) {
				
			}
		}
		for (int i = grades.length; i > 0; i--) {
			int highest = grades[0];
			int spot = 0;
			for (int j = 1; j < i; j++) {
				if (grades[j] > highest) {
					highest = grades[j];
					spot = j;
				}
			}
			int temp = grades[i - 1];
			grades[i - 1] = highest;
			grades[spot] = temp;
			String tempName = lnames[i - 1];
			lnames[i - 1] = lnames[spot];
			lnames[spot] = tempName;
			tempName = fnames[i - 1];
			fnames[i - 1] = fnames[spot];
			fnames[spot] = tempName;
		}
		
		for (int i = 0; i < fnames.length; i++) {
			System.out.println(lnames[i] + ", " + fnames[i] + ", " + grades[i]); 
		}
	}
}
