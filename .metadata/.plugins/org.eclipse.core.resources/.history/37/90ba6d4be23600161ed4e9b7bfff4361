import javax.swing.JOptionPane;

public class ClassOrganizer {
	public static void main(String[] args) {
		String[] names = new String[6];
		for (int i =  0; i < names.length; i++) {
			names[i] = JOptionPane.showInputDialog("What is your name?");
		}
		double[] nums = new double[6];
		for (int i = 0; i < nums.length; i++) {
			nums[i] = Double.parseDouble(JOptionPane.showInputDialog("What is " + names[i] + "'s GPA?"));
		}
		for (int i = nums.length; i > 0; i--) {
			double highest = nums[0];
			int spot = 0;
			for (int j = 1; j < i; j++) {
				if (nums[j] > highest) {
					highest = nums[j];
					spot = j;
				}
			}
			double temp = nums[i - 1];
			nums[i - 1] = highest;
			nums[spot] = temp;
			String tempName = names[i - 1];
			names[i - 1] = names[spot];
			names[spot] = tempName;
		}
		for (int i = 0; i < names.length; i++) {
			System.out.println(names[i] + "'s GPA is " + nums[i]);
		}
	}
}
