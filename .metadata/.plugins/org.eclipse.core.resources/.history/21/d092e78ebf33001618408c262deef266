import javax.swing.JOptionPane;

public class ClassOrganizer {
	public static void main(String[] args) {
		String[] names = new String[6];
		for (int i =  0; i < names.length; i++) {
			names[i] = JOptionPane.showInputDialog("What is your name?");
			System.out.println("Name " + i + " is " + names[i]);
		}
		for (int i = names.length - 1; i > 0; i--) {
			String highest = names[0];
			int spot = 0;
			for (int j = 1; j < i; j++) {
				if (highest.charAt(0) < names[j].charAt(0)) {
					highest = names[j];
					spot = j;
				}
			}
			String temp = names[i] = highest;
			names[spot] = temp;
		}
		for (char i = 'A'; i <= 'Z'; i++) {
			System.out.println(i);
		}
	}
}
